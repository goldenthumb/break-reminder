{
  "name": "break-reminder",
  "version": "0.1.0",
  "description": "",
  "main": "app/mainProcess/main.js",
  "types": "app/mainProcess/main.ts",
  "scripts": {
    "dev": "tsc && cross-env NODE_ENV=development webpack-dev-server --config configs/devserver.js",
    "start": "npm run build && electron app/mainProcess/main.js cross-env NODE_ENV=production electron ./app/main.js",
    "package": "rimraf release && npm run build && electron-builder build --publish never",
    "build": "tsc && cross-env NODE_ENV=production webpack --config configs/build",
    "start-main-dev": "cross-env NODE_ENV=development electron app/mainProcess/main.js"
  },
  "build": {
    "productName": "BreakReminder",
    "appId": "electron.app.BreakReminder",
    "asar": true,
    "mac": {
      "target": [
        "default"
      ],
      "extendInfo": {
        "LSUIElement": 1
      },
      "icon": "./resources/icon.icns"
    },
    "dmg": {
      "title": "BreakReminder",
      "icon": "./resources/icon.icns",
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "./resources/icon.ico"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goldenthumb/break-reminder.git"
  },
  "keywords": [
    "electron",
    "break",
    "time",
    "reminder"
  ],
  "author": "goldenthumb <cyh8914@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/goldenthumb/break-reminder/issues"
  },
  "homepage": "https://github.com/goldenthumb/break-reminder#readme",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-react": "^7.0.0",
    "@types/node": "^12.0.2",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/webpack-env": "^1.13.9",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "cross-spawn": "^6.0.5",
    "css-loader": "^2.1.0",
    "electron": "^5.0.1",
    "electron-builder": "^20.39.0",
    "electron-debug": "^2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@types/moment": "^2.13.0",
    "@types/rc-progress": "^2.2.1",
    "delay": "^4.3.0",
    "event-emitter": "^0.3.5",
    "moment": "^2.24.0",
    "node-notifier": "^5.4.0",
    "query-string": "^6.4.2",
    "rc-progress": "^2.3.0",
    "rc-time-picker": "^3.6.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-icons": "^3.5.0",
    "react-router-dom": "^5.0.0",
    "react-switch": "^4.1.0",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
