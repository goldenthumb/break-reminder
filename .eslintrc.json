{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "rules": {
    "react/no-multi-comp": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-wrap-multilines": 0,
    "react/jsx-closing-tag-location": 0,
    "no-console": 1,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  },
  "plugins": ["react"]
}